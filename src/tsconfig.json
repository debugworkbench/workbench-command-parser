{
    "version": "1.4.1",
    "compilerOptions": {
        "outDir": "../lib",
        "target": "es6",
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": true,
        "removeComments": false,
        "sourceMap": true,
        "noLib": false
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "parser.ts",
        "tokenizer.ts",
        "nodes.ts",
        "builder.ts",
        "help.ts"
    ]
}
