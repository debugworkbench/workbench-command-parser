{
  "name": "workbench-commands",
  "version": "0.1.0",
  "description": "A command interface library for building browser-based CLIs.",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p src",
    "clean": "rimraf lib docs tests/*.js tests/*.map tests/*.d.ts",
    "docs": "typedoc --mode modules --out docs --target es6 src/*.ts",
    "lint": "tslint  -c conf/tslint.json src/*.ts test/*.ts",
    "test": "tsc -p src && tsc -p test && mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/debug-workbench/workbench-commands/"
  },
  "keywords": [
    "CLI",
    "command"
  ],
  "author": "Bruce Mitchener, Jr.",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "tslint": "^3.7.4",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.10"
  }
}
