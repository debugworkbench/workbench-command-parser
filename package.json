{
  "name": "workbench-command-parser",
  "version": "0.1.0",
  "description": "A library providing a command parser.",
  "main": "lib/index.js",
  "scripts": {
    "build:src": "tsc -p src",
    "build:test": "tsc -p test/src",
    "build": "npm run build:src && npm run build:test",
    "clean": "rimraf lib docs test/build",
    "docs": "typedoc --mode file --readme src/index.md --out docs --target es6 src/*.ts",
    "lint": "tslint  -c conf/tslint.json src/*.ts test/*.ts",
    "test": "npm run build && mocha test/build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/debug-workbench/workbench-command-parser/"
  },
  "keywords": [
    "CLI",
    "command"
  ],
  "author": "Bruce Mitchener, Jr.",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "tslint": "^3.7.4",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.10"
  }
}
